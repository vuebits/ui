{
  "name": "@vuebits/ui",
  "version": "0.4.0",
  "description": "Vue 3 components library",
  "main": "./dist/vuebits-ui.umd.js",
  "module": "./dist/vuebits-ui.es.js",
  "directories": {
    "test": "tests"
  },
  "author": "Vuebits (https://github.com/vuebits)",
  "contributors": [
    "Szczepan Masny <szczepanmasny@gmail.com> (https://github.com/szczepanmasny)",
    "Agnieszka Ziomek <poziomkaa@gmail.com> (https://github.com/Ziomeka)"
  ],
  "homepage": "https://github.com/vuebits/ui",
  "keywords": [
    "ui",
    "design pattern",
    "vuejs",
    "vue3",
    "vue ui",
    "vuejs ui",
    "vue3 ui"
  ],
  "files": [
    "dist",
    "styles",
    "src",
    "types"
  ],
  "types": "./types/index.d.ts",
  "license": "MIT",
  "scripts": {
    "docs:serve": "vite --config vite-docs.config.js",
    "docs:build": "vite build --config vite-docs.config.js",
    "docs:preview": "vite preview --config vite-docs.config.js",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "test:unit": "jest",
    "lint": "eslint --ext .js,.ts,.vue .",
    "component:create": "node ./utils/component/scripts/create",
    "precommit": "lint-staged",
    "prepush": "npm run lint && npm run stylelint && npm run test:unit",
    "stylelint": "npx stylelint '**/*.scss'",
    "stylelint:fix": "npx stylelint '**/*.scss' --fix",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@fontsource/ubuntu": "^4.2.2",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "core-js": "^3.6.5",
    "dateformat": "^5.0.1",
    "highlight.js": "^10.6.0",
    "vue": "^3.2.19",
    "vue-router": "^4.0.11"
  },
  "devDependencies": {
    "@originjs/vite-plugin-commonjs": "^1.0.1",
    "@types/dateformat": "^3.0.1",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vitejs/plugin-vue": "^2.0.1",
    "@vue/compiler-sfc": "^3.2.26",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^2.0.0-0",
    "@vue/vue3-jest": "^27.0.0-alpha.4",
    "babel-jest": "^27.4.6",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^7.0.0-0",
    "fs-extra": "^9.1.0",
    "inquirer": "^8.0.0",
    "jest": "^27.4.7",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^12.1.7",
    "raw-loader": "^4.0.2",
    "replace-in-file": "^6.2.0",
    "sass": "^1.43.4",
    "simple-git-hooks": "^2.7.0",
    "stylelint": "^14.2.0",
    "stylelint-config-recommended-scss": "^5.0.2",
    "stylelint-scss": "^4.1.0",
    "ts-jest": "^27.1.2",
    "typescript": "^4.4.3",
    "vite": "^2.7.2",
    "vite-plugin-env-compatible": "^1.1.1",
    "vite-plugin-html": "2.1.1",
    "vue-tsc": "^0.30.2"
  },
  "lint-staged": {
    "*.{js,vue,ts}": [
      "eslint",
      "git add"
    ],
    "*.{scss, css}": [
      "stylelint",
      "git add"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "npm run precommit",
    "pre-push": "npm run prepush"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.vuebits.com/vuebits/front-ui.git"
  }
}
